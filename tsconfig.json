{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "lib": [
            "es6",
            "dom"
        ],
        "moduleResolution": "node",
        "declaration": false,
        "noImplicitAny": false,
        "jsx": "react",
        "sourceMap": true,
        "noLib": false,
        "suppressImplicitAnyIndexErrors": true
    },
    "compileOnSave": false,
    "exclude": [
        "node_modules",
        "vendor"
    ]
}